@prefix tom: <https://w3id.org/toRoll/vocs/taginOM/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

tom: a skos:ConceptScheme ;
    dct:title "Tagin"@de;
    skos:hasTopConcept tom:tagin, tom:1tag, tom:alef_1000 .

tom:tagin a  skos:Concept ;
        skos:narrower tom:1tag ;
        skos:prefLabel "tagin"@de ;
        skos:topConceptOf tom: .

tom:1tag a skos:Concept ;
        skos:broader tom:tagin ;
        skos:narrower tom:alef_1000 ; 
        skos:prefLabel "1 tag"@de .

tom:alef_1000 a skos:Concept ;
        skos:broader tom:1tag ;
        skos:prefLabel "alef_1000"@de .
